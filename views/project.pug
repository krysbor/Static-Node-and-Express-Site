extends layout.pug

block content
  nav.grid-container.portfolio-breadcrumb
    a(href='/') ‚Üê Back
  article.grid-container.portfolio-intro.portfolio-project
    .grid-x
      .cell.small-6.medium-9.large-9


        h1= object.project_name


        p.lead.text-light= object.description

      .cell.portfolio-meta.small-6.medium-2.large-3
        h6 Technologies


        ul
        each technology in object.technologies
          li= technology


        a.button.expanded(href= object.live_link) Live Demo
        a.button.expanded(href= object.github_link) GitHub Repo


    img.image(src= object.image_urls[1])
    img.image(src='https://placehold.it/1200x550')
    img.image(src='https://placehold.it/1200x550')